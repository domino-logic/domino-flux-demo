{
  "name": "domino-flux-demo",
  "version": "0.0.0",
  "author": "Brice LEROY",
  "scripts": {
    "start": "npm run watch & npm run serve",
    "watch": "watchify -v -o js/bundle.js js/app.js",
    "serve": "ws --spa index.html",
    "build": "browserify . -t [envify --NODE_ENV production] | uglifyjs -cm > js/bundle.min.js",
    "test": "jest"
  },
  "description": "Example Domino-Flux architecture.",
  "repository": "https://github.com/facebook/flux",
  "main": "js/app.js",
  "dependencies": {
    "classnames": "^2.2.5",
    "flux": "^2.1.1",
    "keymirror": "^0.1.1",
    "object-assign": "^4.1.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^6.2.0",
    "envify": "^3.0.0",
    "jest-cli": "^0.4.3",
    "local-web-server": "^1.2.3",
    "reactify": "^0.15.2",
    "uglify-js": "~2.4.15",
    "watchify": "^2.1.1"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "react",
            "es2015"
          ]
        }
      ]
    ]
  },
  "jest": {
    "rootDir": "./js"
  }
}
